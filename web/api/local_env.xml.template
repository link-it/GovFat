	<property name="required_lib_fatturaPA" location="${daoApiFatturaPA}/deploy/lib" />

	<property name="api_fatturaPA" value="${daoApiFatturaPA}/dist/fatturaPA_api.jar" />

	<!-- Jar del livello di persistenza scelto (default jdbc) -->
	<property name="impl_fatturaPA" location="../../dao/impl/jdbc/dist/fatturaPA_jdbc_impl.jar" />

</project>
