<?xml version="1.0" encoding="UTF-8" standalone="no"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.govmix.org/proxy/fatturapa/orm" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.govmix.org/proxy/fatturapa/orm">

	


	<xsd:simpleType name="IdentificativoSdIType">
		<xsd:restriction base="xsd:integer">
			<xsd:totalDigits value="12"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NomeFileType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="FormatoArchivioInvioFatturaType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="XML"/>
			<xsd:enumeration value="P7M"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="MessageIdType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="14"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String60LatinType">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,60}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String80LatinType">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,80}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CodiceType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="28"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CodiceDestinatarioType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9]{6,7}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="TipoDocumentoType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="4"/>
			<xsd:enumeration value="TD01">
				<xsd:annotation>
					<xsd:documentation>Fattura</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD02">
				<xsd:annotation>
					<xsd:documentation>Acconto / anticipo su fattura
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD03">
				<xsd:annotation>
					<xsd:documentation>Acconto / anticipo su parcella
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD04">
				<xsd:annotation>
					<xsd:documentation>Nota di credito</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD05">
				<xsd:annotation>
					<xsd:documentation>Nota di debito</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD06">
				<xsd:annotation>
					<xsd:documentation>Parcella</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="OperazioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ProxyPagamentoIva"/>
			<xsd:enumeration value="QueryInserimentoFattura"/>
			<xsd:enumeration value="ReadStatoFattura"/>
			<xsd:enumeration value="QueryOperazioneContabile"/>
			<xsd:enumeration value="ProxyDatiFattura"/>
			<xsd:enumeration value="ProxyInserimentoFattura"/>
			<xsd:enumeration value="ProxyOperazioneContabile"/>
			<xsd:enumeration value="ReadDownloadDocumento"/>
			<xsd:enumeration value="QueryDatiFattura"/>
			<xsd:enumeration value="QueryPagamentoIva"/>
			<xsd:enumeration value="ReadElencoMovimentiErarioIva"/>
			<xsd:enumeration value="QueryInterrogazioneEsiti"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="DivisaType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="DataFatturaType">
		<xsd:restriction base="xsd:date">
			<xsd:minInclusive value="1970-01-01"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String10Type">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="(\p{IsBasicLatin}{1,10})"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String20Type">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="(\p{IsBasicLatin}{1,20})"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="PasswordType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9]{35}"/> <!-- MD5 della password -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="UsernameType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9]{4,50}"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="NaturaSpesaType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="2"/>
			<xsd:enumeration value="CO"/>
			<xsd:enumeration value="CA"/>
			<xsd:enumeration value="NA"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="RispedizioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="StatoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="S_OK"/>
			<xsd:enumeration value="S_ERRORE"/>
			<xsd:enumeration value="AS_PRESA_IN_CARICO"/>
			<xsd:enumeration value="AS_ERRORE_PRESA_IN_CARICO"/>
			<xsd:enumeration value="AS_OK"/>
			<xsd:enumeration value="AS_ERRORE"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="TipoOperazionePccType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PROXY"/>
			<xsd:enumeration value="READ"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="CausaleType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ATTLIQ"/>
			<xsd:enumeration value="CONT"/>
			<xsd:enumeration value="ATTNC"/>
			<xsd:enumeration value="NCRED"/>
			<xsd:enumeration value="PAGTERZI"/>
			<xsd:enumeration value="IVARC"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="StatoDebitoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LIQ"/>
			<xsd:enumeration value="NOLIQ"/>
			<xsd:enumeration value="SOSP"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="EsitoTrasmissioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="OK"/>
			<xsd:enumeration value="KO"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="StatoEsitoTrasmissioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PRESENTE"/>
			<xsd:enumeration value="NON_PRESENTE"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="TipoErroreType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TRASMISSIONE"/>
			<xsd:enumeration value="ELABORAZIONE"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="NomePccOperazioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ConsultazioneTracce"/>
			<xsd:enumeration value="DatiFattura"/>
			<xsd:enumeration value="PagamentoIva"/>
			<xsd:enumeration value="InserimentoFattura"/>
			<xsd:enumeration value="StatoFattura"/>
			<xsd:enumeration value="ElencoMovimentiErarioIva"/>
			<xsd:enumeration value="DownloadDocumento"/>
			<xsd:enumeration value="OperazioneContabile_CP"/>
			<xsd:enumeration value="OperazioneContabile_CO"/>
			<xsd:enumeration value="OperazioneContabile_CS"/>
			<xsd:enumeration value="OperazioneContabile_CCS"/>
			<xsd:enumeration value="OperazioneContabile_CPS"/>
			<xsd:enumeration value="OperazioneContabile_CSPC"/>
			<xsd:enumeration value="OperazioneContabile_SP"/>
			<xsd:enumeration value="OperazioneContabile_RF"/>
			<xsd:enumeration value="OperazioneContabile_SC"/>
			<xsd:enumeration value="OperazioneContabile_RC"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="TipoOperazioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CO"/>
			<xsd:enumeration value="CP"/>
			<xsd:enumeration value="SP"/>
			<xsd:enumeration value="CS"/>
			<xsd:enumeration value="RF"/>
			<xsd:enumeration value="RC"/>
			<xsd:enumeration value="CCS"/>
			<xsd:enumeration value="SC"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="Amount2DecimalType">
		<xsd:restriction base="xsd:decimal">
			<xsd:pattern value="[\-]?[0-9]{1,11}\.[0-9]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String100LatinType">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,100}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String200LatinType">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,200}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String1000LatinType">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,1000}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="FormatoTrasmissioneType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="5"/>
			<xsd:enumeration value="FPA12"/>
			<xsd:enumeration value="FPR12"/>
			<xsd:enumeration value="SDI11"/>
			<xsd:enumeration value="SDI10"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="EsitoCommittenteType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EC01">
				<xsd:annotation>
					<xsd:documentation>EC01 = ACCETTAZIONE</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EC02">
				<xsd:annotation>
					<xsd:documentation>EC02 = RIFIUTO</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ScartoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EN00">
				<xsd:annotation>
					<xsd:documentation>EN00 = NOTIFICA NON CONFORME AL FORMATO
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EN01">
				<xsd:annotation>
					<xsd:documentation>EN01 = NOTIFICA NON AMMISSIBILE
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="EsitoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="IN_ELABORAZIONE_ACCETTATO"/>
			<xsd:enumeration value="IN_ELABORAZIONE_RIFIUTATO"/>
			<xsd:enumeration value="INVIATA_ACCETTATO"/>
			<xsd:enumeration value="INVIATA_RIFIUTATO"/>
			<xsd:enumeration value="SCARTATA_ACCETTATO"/>
			<xsd:enumeration value="SCARTATA_RIFIUTATO"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="StatoConsegnaType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NON_CONSEGNATA"/>
			<xsd:enumeration value="IN_RICONSEGNA"/>
			<xsd:enumeration value="ERRORE_CONSEGNA"/>
			<xsd:enumeration value="CONSEGNATA"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="StatoProtocollazioneType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NON_PROTOCOLLATA"/>
			<xsd:enumeration value="PROTOCOLLATA_IN_ELABORAZIONE"/>
			<xsd:enumeration value="ERRORE_PROTOCOLLAZIONE"/>
			<xsd:enumeration value="PROTOCOLLATA"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="StatoInserimentoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NON_INSERITO"/>
			<xsd:enumeration value="ERRORE_INSERIMENTO"/>
			<xsd:enumeration value="INSERITO"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NazioneType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="id-fattura">
		<xsd:sequence>
			<xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
			<xsd:element name="posizione" type="xsd:positiveInteger"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="id-lotto">
		<xsd:sequence>
			<xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-pagamento">
		<xsd:sequence>
			<xsd:element name="idPagamento" type="xsd:long"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-scadenza">
		<xsd:sequence>
			<xsd:element name="idScadenza" type="xsd:long"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-notifica-decorrenza-termini">
		<xsd:sequence>
			<xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="id-dipartimento">
		<xsd:sequence>
			<xsd:element name="codice" type="CodiceDestinatarioType"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<!--xsd:complexType name="id-pcc-ufficio">
		<xsd:sequence>
			<xsd:element name="codiceUfficioPcc" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType-->
	
	<xsd:complexType name="id-utente">
		<xsd:sequence>
			<xsd:element name="username" type="UsernameType"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="id-ente">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-protocollo">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-registro">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-operazione">
		<xsd:sequence>
			<xsd:element name="nome" type="NomePccOperazioneType"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-rispedizione">
		<xsd:sequence>
			<xsd:element name="codiceErrore" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-contabilizzazione">
		<xsd:sequence>
			<xsd:element name="idImporto" type="xsd:string"/>
			<xsd:element name="sistemaRichiedente" type="xsd:string"/>
			<xsd:element name="idFattura" type="id-fattura"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="user-role">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ADMIN"/>
			<xsd:enumeration value="DEPT_ADMIN"/>
			<xsd:enumeration value="USER"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="TipoComunicazioneType">
		<xsd:restriction base="xsd:string">
<!--		<xsd:enumeration value="FATTURA_INGRESSO" /> -->
			<xsd:enumeration value="FATTURA_USCITA"/>
			<xsd:enumeration value="NOTIFICA_SCARTO"/>
			<xsd:enumeration value="RICEVUTA_CONSEGNA"/>
			<xsd:enumeration value="NOTIFICA_MANCATA_CONSEGNA"/>
			<xsd:enumeration value="ATTESTAZIONE_TRASMISSIONE_FATTURA"/>
			<xsd:enumeration value="NOTIFICA_ESITO_COMMITTENTE"/>
			<xsd:enumeration value="NOTIFICA_DECORRENZA_TERMINI_TRASMITTENTE"/>
<!--		<xsd:enumeration value="NOTIFICA_DECORRENZA_TERMINI_DESTINATARIO" /> -->
			<xsd:enumeration value="AVVENUTA_TRASMISSIONE_IMPOSSIBILITA_RECAPITO"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="id-comunicazione">
		<xsd:sequence>
			<xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
			<xsd:element name="tipoComunicazione" type="TipoComunicazioneType"/>
			<xsd:element name="progressivo" type="xsd:positiveInteger"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="user-type">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="INTERNO"/>
			<xsd:enumeration value="ESTERNO"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="id-dipartimento-property">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string"/>
			<xsd:element name="id-ente" type="id-ente"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-registro-property">
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string"/>
			<xsd:element name="id-protocollo" type="id-protocollo"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-traccia">
		<xsd:sequence>
			<xsd:element name="idTraccia" type="xsd:long"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-trasmissione">
		<xsd:sequence>
			<xsd:element name="idTrasmissione" type="xsd:long"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-trasmissione-esito">
		<xsd:sequence>
			<xsd:element name="idTrasmissioneEsito" type="xsd:long"/>
		</xsd:sequence>
	</xsd:complexType>
	
	
	<xsd:complexType name="ComunicazioneSdi">
		
		<xsd:sequence>
			<xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
			<xsd:element name="tipoComunicazione" type="TipoComunicazioneType"/>
			<xsd:element name="progressivo" type="xsd:positiveInteger"/>
			<xsd:element name="dataRicezione" type="DataFatturaType"/>
			<xsd:element minOccurs="0" name="nomeFile" type="NomeFileType"/> <!-- Temporaneamente -->
			<xsd:element name="contentType" type="xsd:string"/>
			<xsd:element name="rawData" type="xsd:base64Binary"/>
			<xsd:element name="statoConsegna" type="StatoConsegnaType"/>
			<xsd:element minOccurs="0" name="dataConsegna" type="xsd:dateTime"/>
			<xsd:element minOccurs="0" name="dettaglioConsegna" type="xsd:string"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="Metadato"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="Metadato">
		
		<xsd:sequence>
			<xsd:element name="nome" type="xsd:string"/>
			<xsd:element name="valore" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	
	<xsd:complexType name="LottoFatture">
		
		<xsd:sequence>
			<xsd:element name="formatoTrasmissione" type="FormatoTrasmissioneType"/>
			<xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
			<xsd:element name="nomeFile" type="NomeFileType"/>
			<xsd:element name="formatoArchivioInvioFattura" type="FormatoArchivioInvioFatturaType"/>
			<xsd:element name="messageId" type="MessageIdType"/>
			<xsd:element minOccurs="0" name="cedentePrestatoreDenominazione" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cedentePrestatoreNome" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cedentePrestatoreCognome" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cedentePrestatoreCodice" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cedentePrestatorePaese" type="NazioneType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cedentePrestatoreCodiceFiscale" type="CodiceType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cessionarioCommittenteDenominazione" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cessionarioCommittenteNome" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cessionarioCommittenteCognome" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cessionarioCommittenteCodice" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cessionarioCommittentePaese" type="NazioneType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="cessionarioCommittenteCodiceFiscale" type="CodiceType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="terzoIntermediarioOSoggettoEmittenteDenominazione" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="terzoIntermediarioOSoggettoEmittenteNome" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="terzoIntermediarioOSoggettoEmittenteCognome" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="terzoIntermediarioOSoggettoEmittenteCodice" type="String80LatinType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="terzoIntermediarioOSoggettoEmittentePaese" type="NazioneType">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="terzoIntermediarioOSoggettoEmittenteCodiceFiscale" type="CodiceType">
				
			</xsd:element>
			<xsd:element name="codiceDestinatario" type="CodiceDestinatarioType"/>
