<project>
	<name>FatturaPA risorse</name>
	<groupId>fatturapa</groupId>
	<artifactId>risorse</artifactId>
	<version>1.1.pab</version>
	<modelVersion>4.0.0</modelVersion>
	<build>
		<filters>
			<filter>../filters/${env}.filter.properties</filter>
		</filters>
		<resources>
			<resource>
				<directory>resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
	</build>
</project>
