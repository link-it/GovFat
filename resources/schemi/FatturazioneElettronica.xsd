		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PA">
				<xsd:annotation>
					<xsd:documentation>Fattura PA</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="B2B">
				<xsd:annotation>
					<xsd:documentation>Fattura B2B</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
