		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ESTERO"/>
			<xsd:enumeration value="PEC"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="TipoDocumentoType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="4" />
			<xsd:enumeration value="TD01">
				<xsd:annotation>
					<xsd:documentation>Fattura</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD02">
				<xsd:annotation>
					<xsd:documentation>Acconto / anticipo su fattura
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD03">
				<xsd:annotation>
					<xsd:documentation>Acconto / anticipo su parcella
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD04">
				<xsd:annotation>
					<xsd:documentation>Nota di credito</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD05">
				<xsd:annotation>
					<xsd:documentation>Nota di debito</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD06">
				<xsd:annotation>
					<xsd:documentation>Parcella</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TD20">
				<xsd:annotation>
					<xsd:documentation>Autofattura</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDXX">
				<xsd:annotation>
					<xsd:documentation>Sconosciuto</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
