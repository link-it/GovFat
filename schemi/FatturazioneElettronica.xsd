    <xsd:simpleType name="IdentificativoSdIType">
        <xsd:restriction base="xsd:integer">
            <xsd:totalDigits value="12"/>
        </xsd:restriction>
    </xsd:simpleType>
     
    <xsd:simpleType name="NomeFileType">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="50"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="MessageIdType">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="14"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="String60LatinType">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,60}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="String80LatinType">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,80}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="CodiceType">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="28" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="CodiceDestinatarioType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z0-9]{6}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="TipoDocumentoType">
        <xsd:restriction base="xsd:string">
            <xsd:length value="4" />
            <xsd:enumeration value="TD01">
                <xsd:annotation>
                    <xsd:documentation>Fattura</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TD02">
                <xsd:annotation>
                    <xsd:documentation>Acconto / anticipo su fattura</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TD03">
                <xsd:annotation>
                    <xsd:documentation>Acconto / anticipo su parcella</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TD04">
                <xsd:annotation>
                    <xsd:documentation>Nota di credito</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TD05">
                <xsd:annotation>
                    <xsd:documentation>Nota di debito</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TD06">
                <xsd:annotation>
                    <xsd:documentation>Parcella</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="DivisaType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{3}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="DataFatturaType">
        <xsd:restriction base="xsd:date">
            <xsd:minInclusive value="1970-01-01" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="String10Type">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:pattern value="(\p{IsBasicLatin}{1,10})" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="String20Type">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:pattern value="(\p{IsBasicLatin}{1,20})" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="PasswordType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9]{35}"/> <!-- MD5 della password -->
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="UsernameType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9]{4,20}"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="Amount2DecimalType">
        <xsd:restriction base="xsd:decimal">
            <xsd:pattern value="[\-]?[0-9]{1,11}\.[0-9]{2}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="String100LatinType">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,100}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="String200LatinType">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}]{1,200}" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="FormatoTrasmissioneType">
        <xsd:restriction base="xsd:string">
            <xsd:length value="5" />
            <xsd:enumeration value="SDI11"/>
            <xsd:enumeration value="SDI10"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="EsitoCommittenteType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="EC01">
                <xsd:annotation>
                    <xsd:documentation>EC01 = ACCETTAZIONE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="EC02">
                <xsd:annotation>
                    <xsd:documentation>EC02 = RIFIUTO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="ScartoType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="EN00">
                <xsd:annotation>
                    <xsd:documentation>EN00 = NOTIFICA NON CONFORME AL FORMATO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="EN01">
                <xsd:annotation>
                    <xsd:documentation>EN01 = NOTIFICA NON AMMISSIBILE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="EsitoType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ESITO_ACCETTAZIONE_IN_TRASMISSIONE_SDI"/>
            <xsd:enumeration value="ESITO_RIFIUTO_IN_TRASMISSIONE_SDI"/>
            <xsd:enumeration value="ESITO_ACCETTAZIONE_SCARTATO_SDI"/>
            <xsd:enumeration value="ESITO_RIFIUTO_SCARTATO_SDI"/>
            <xsd:enumeration value="ESITO_ACCETTAZIONE_INVIATO_SDI"/>
            <xsd:enumeration value="ESITO_RIFIUTO_INVIATO_SDI"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="StatoFatturaPassivaType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="IN_CONSEGNA_ENTE"/>
            <xsd:enumeration value="ERRORE_CONSEGNA_ENTE"/>
            <xsd:enumeration value="CONSEGNATA_ENTE"/>
            <xsd:enumeration value="ESITO_ACCETTAZIONE_IN_TRASMISSIONE_SDI"/>
            <xsd:enumeration value="ESITO_RIFIUTO_IN_TRASMISSIONE_SDI"/>
            <xsd:enumeration value="ESITO_ACCETTAZIONE_SCARTATO_SDI"/>
            <xsd:enumeration value="ESITO_RIFIUTO_SCARTATO_SDI"/>
            <xsd:enumeration value="ESITO_ACCETTAZIONE_INVIATO_SDI"/>
            <xsd:enumeration value="ESITO_RIFIUTO_INVIATO_SDI"/>
            <xsd:enumeration value="DECORRENZA_TERMINI"/>
            <xsd:enumeration value="IN_LIQUIDAZIONE"/>
            <xsd:enumeration value="LIQUIDATA"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="StatoFatturaAttivaType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="IN_TRASMISSIONE_SDI"/>
            <xsd:enumeration value="ERRORE_TRASMISSIONE_SDI"/>
            <xsd:enumeration value="TRASMESSA_SDI"/>
            <xsd:enumeration value="SCARTATA_SDI"/>
            <xsd:enumeration value="MANCATA_CONSEGNA_DESTINATARIO"/>
            <xsd:enumeration value="IMPOSSIBILE_RECAPITO_DESTINATARIO"/>
            <xsd:enumeration value="CONSEGNATA_DESTINATARIO"/>
            <xsd:enumeration value="ACCETTATA_DESTINATARIO"/>
            <xsd:enumeration value="RIFIUTATA_DESTINATARIO"/>
            <xsd:enumeration value="DECORRENZA_TERMINI"/>
        </xsd:restriction>    
    </xsd:simpleType>
    
    <xsd:simpleType name="StatoInvioEsitoType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="DA_INVIARE"/>
            <xsd:enumeration value="INVIATO"/>
            <xsd:enumeration value="ERRORE_INVIO"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="NazioneType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{2}" />
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="CodiceErroreScartoType">
        <xsd:restriction base="xsd:string">
            <xsd:length value="5"/>
        </xsd:restriction>
    </xsd:simpleType>
    
<!-- IDs -->    
        
    <xsd:complexType name="id-fattura-uscita">
        <xsd:sequence>
            <xsd:element name="identificativoSdi" type="IdentificativoSdIType" />
            <xsd:element name="nomeFile" type="NomeFileType" />
        </xsd:sequence>    
    </xsd:complexType>
    
    <xsd:complexType name="id-fattura-entrata">
        <xsd:sequence>
            <xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
            <xsd:element name="nomeFile" type="NomeFileType" />
            <xsd:element name="posizione" type="xsd:positiveInteger"/>
        </xsd:sequence>    
    </xsd:complexType>
    
    <xsd:complexType name="id-notifica-decorrenza-termini">
        <xsd:sequence>
            <xsd:element name="identificativoSdi" type="IdentificativoSdIType"/>
        </xsd:sequence>    
    </xsd:complexType>
    
    <xsd:complexType name="id-dipartimento">
        <xsd:sequence>
            <xsd:element name="codice" type="CodiceDestinatarioType"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="id-utente">
        <xsd:sequence>
            <xsd:element name="username" type="UsernameType"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="id-ente">
        <xsd:sequence>
            <xsd:element name="codiceFiscale" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
        
